#include <iostream>

using namespace std;

int main(){
	int num, temp, sum = 0;
	
	cout << "Enter a Positive Number : " << endl;cin >> num;
	temp = num;

    while(num) {
        int k = num % 10;
        sum += k;
        num /= 10;
    }
    //sum = 10;
	if(temp % sum == 0)
		cout << "number is divisible" << endl;
	else
		cout << "number is not divisible" << endl;
	
	system("pause");
}
